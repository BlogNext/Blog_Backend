                "produces": [
                    "application/json"
                ],
                "tags": [
                    "前台-博客"
                ],
                "parameters": [
                    {
                        "type": "integer",
                        "description": "一页多少条，默认值5",
                        "name": "per_page",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "排序维度，默认值browse_total",
                        "name": "sort_dimension",
                        "in": "query"
                    }
